<script>
	var showGenericError = function (errorData) {
		$('#errorMessageField')
			.append($('<span>')
				.addClass("text-error")
				.html(errorData.defaultMessage));
	};

	var loginFormErrorCodes = {
		username: {
			blank: ''
		},
		password: {
			blank: ''
		},
		generic : {
			invalidCredentials: {
				message: '',
				action : showGenericError
			},
			accountLocked     : {
				message: '',
				action : showGenericError
			}
		}
	};
</script>

